
<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <table class="table">
      <thead>
        <tr>
          <th>Date</th>
          <th>Venue</th>
          <th>City</th>
          <th>Url</th>
        </tr>
      </thead>
      <tbody>
      <% @shows.each do |show| %>
        <tr>
          <td><%= show.date.strftime("%m/%d") %></td>
          <td><%= show.description %></td>
          <td><%= show.location %></td>
          <td><a href="<%= show.url %>">More Info</a></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>